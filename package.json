{
  "name": "twitch-chat-snake",
  "version": "1.0.0",
  "description": "A twitch chat based snake game powered by electron typescript",
  "author": "nkorai",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "prettier . --write && TIMING=1 eslint -c .eslintrc --ext .ts ./src --fix",
    "start": "npm run build && npx webpack --config webpack.config.js && electron ./dist/main.js",
    "clean": "rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "electron-store": "^8.1.0",
    "lodash": "^4.17.21",
    "progressbar.js": "^1.1.0",
    "tmi.js": "^1.8.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.196",
    "@types/progressbar.js": "^1.1.3",
    "@types/tmi.js": "^1.8.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "css-loader": "^6.8.1",
    "electron": "^24.3.0",
    "eslint": "^8.0.0",
    "eslint-config-async": "^2.0.1",
    "eslint-config-prettier": "^8.9.0",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "3.0.0",
    "prettier-plugin-organize-imports": "^3.2.3",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^4.9.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
